{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/znck/AmazonMwsComplete"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "aws/aws-sdk-php": "^3.26",
    "caponica/amazon-mws-complete": "0.2",
    "jenssegers/mongodb": "^3.2",
    "laravel/framework": "5.4.*",
    "laravel/tinker": "~1.0",
    "predis/predis": "^1.1",
    "spatie/laravel-server-monitor": "^1.2",
    "znck/transform": "dev-master",
    "znck/trust": "dev-master"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.3",
    "doctrine/dbal": "^2.5",
    "fzaninotto/faker": "~1.4",
    "laravel/dusk": "^1.0",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ],
    "generate": [
      "php artisan ide-helper:models -F .phpstorm.meta.php/models.php",
      "php artisan ide-helper:generate -- .phpstorm.meta.php/facade",
      "php artisan ide-helper:meta -F .phpstorm.meta.php/laravel.php"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
